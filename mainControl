--project beginning
local basalt = require("basalt")

local mainframe = basalt.createFrame("mainFrame"):show()
local basespeakerbutton = mainFrame
:addButton("speakerbutton")
:setPosition(4,4)
:setText("Speaker Control")
:onClick(
  function()
    shell.run(speakercontrol)
  end)

basalt.autoUpdate()
